<?xml version="1.0" encoding="utf-8"?>
<familyset version="22">
    <!-- first font is default -->
    <family name="sans-serif">
        <font weight="50" style="normal">poppins-latin-300.ttf</font>
        <font weight="50" style="italic">poppins-latin-300.ttf</font>
        <font weight="200" style="normal">poppins-latin-300.ttf</font>
        <font weight="200" style="italic">poppins-latin-300.ttf</font>
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
        <font weight="300" style="italic">poppins-latin-regular.ttf</font>
        <font weight="400" style="normal">poppins-latin-500.ttf</font>
        <font weight="400" style="italic">poppins-latin-500.ttf</font>
        <font weight="800" style="normal">poppins-latin-600.ttf</font>
        <font weight="800" style="italic">poppins-latin-600.ttf</font>
        <font weight="600" style="normal">poppins-latin-700.ttf</font>
        <font weight="600" style="italic">poppins-latin-700.ttf</font>
    </family>

    <!-- Note that aliases must come after the fonts they reference. -->
    <alias name="sans-serif-thin" to="sans-serif" weight="50" />
    <alias name="sans-serif-light" to="sans-serif" weight="200" />
    <alias name="sans-serif-medium" to="sans-serif" weight="400" />
    <alias name="sans-serif-black" to="sans-serif" weight="800" />
    <alias name="arial" to="sans-serif" />
    <alias name="helvetica" to="sans-serif" />
    <alias name="tahoma" to="sans-serif" />
    <alias name="verdana" to="sans-serif" />

    <family name="sans-serif-condensed">
        <font weight="200" style="normal">poppins-latin-300.ttf</font>
        <font weight="200" style="italic">poppins-latin-300.ttf</font>
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
        <font weight="300" style="italic">poppins-latin-regular.ttf</font>
        <font weight="400" style="normal">poppins-latin-500.ttf</font>
        <font weight="400" style="italic">poppins-latin-500.ttf</font>
    </family>
    <alias name="sans-serif-condensed-light" to="sans-serif-condensed" weight="300" />

    <family name="serif">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
        <font weight="300" style="italic">poppins-latin-regular.ttf</font>
        <font weight="600" style="normal">poppins-latin-500.ttf</font>
        <font weight="600" style="italic">poppins-latin-500.ttf</font>
    </family>
    <alias name="times" to="serif" />
    <alias name="times new roman" to="serif" />
    <alias name="palatino" to="serif" />
    <alias name="georgia" to="serif" />
    <alias name="baskerville" to="serif" />
    <alias name="goudy" to="serif" />
    <alias name="fantasy" to="serif" />
    <alias name="ITC Stone Serif" to="serif" />

    <family name="monospace">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
    </family>
    <alias name="sans-serif-monospace" to="monospace" />
    <alias name="monaco" to="monospace" />

    <family name="serif-monospace">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
    </family>
    <alias name="courier" to="serif-monospace" />
    <alias name="courier new" to="serif-monospace" />

    <family name="casual">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
    </family>

    <family name="cursive">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
        <font weight="600" style="normal">poppins-latin-500.ttf</font>
    </family>

    <family name="sans-serif-smallcaps">
        <font weight="300" style="normal">poppins-latin-regular.ttf</font>
    </family>
</familyset>